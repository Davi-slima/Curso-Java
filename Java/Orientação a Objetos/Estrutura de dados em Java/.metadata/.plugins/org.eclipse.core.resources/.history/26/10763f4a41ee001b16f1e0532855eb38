package application.entities;

public class Lista<T> {
	
	No<T> refEntradaLista;
	
	public Lista() {
		this.refEntradaLista = null;
	}

	public int size() {
		int tamanhoLista = 0;
		
		No<T> refAux = refEntradaLista;
		return tamanhoLista;
	}
	
	public boolean isEmpty() {
		return refEntradaLista == null ? true : false;
	}
	
}
