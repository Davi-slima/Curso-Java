package application.entities;

public class No<T> {
	
	private Object obj;
	private T refNo;
	
	public No() {
	}
	
	public No(Object obj) {
		this.refNo = null;
		this.obj = obj;
	}

	public Object getObj() {
		return obj;
	}

	public void setObj(Object obj) {
		this.obj = obj;
	}

	public T getRefNo() {
		return refNo;
	}

	public void setRefNo(T refNo) {
		this.refNo = refNo;
	}

	@Override
	public String toString() {
		return "No { obj = " + obj + " }";
	}
	
	
}
